<h1> New stock entry</h1>

<%= form_for :stock, url: stocks_path do |f| %>
  <p>
    <%= f.label :ticker %><br>
    <%= f.text_field :ticker %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', stocks_path %>